   "id": "e9c884f8",
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "pdf_file = './pdfs/Data_Pipelines_with_Apache.pdf' # replace with the pdf to convert\n",
    "\n",
    "with pdfplumber.open(pdf_file) as pdf:\n",
    "    markdown_text = \"\"\n",
    "    for page in pdf.pages:\n",
    "        markdown_text += page.extract_text()\n",
    "        \n",
    "with open(\"./md/output.md\", \"w\",encoding=\"utf-8\") as file:\n",
    "    file.write(markdown_text)"
   ]
